
call "Methods.sim"

CLIENTSPEC = []

__loadClientSpec()
	

block __loadClientSpec()
	clientDump = SERVER["HTTP_USER_AGENT"]
	startIndex = 0 endIndex = 0
	for a = 0 in clientDumpLen {
		if clientDump[a] = "(" { startIndex = a }
		if clientDump[a] = ")" { endIndex = a }
	}
	CLIENTSPEC["OS"] = clientDump