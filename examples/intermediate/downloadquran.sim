call "simple/internet/Download.sim" call "simple/io/File.sim" block main() display "Download QURAN chapters " +crlf +"Azeez Adewale" +crlf +"March 28 2018" +crlf QURAN_START_CHAPTER =1 QURAN_END_CHAPTER =114 SAVE_LOCATION ="~\Music\QURAN" for a =QURAN_START_CHAPTER to QURAN_END_CHAPTER if (a <100)a ="0"+a end display "Downloading chapter "+a +" : " try d =new Download("https://server6.mp3quran.net/thubti/"+a+".mp3") d.saveLocation(SAVE_LOCATION+"/"+a+".mp3") d.execute()display "SUCCESSFULL"+crlf catch display __err__display "FAILED"+crlf end end