call "fulltick/widget/FInput.sim" call "fulltick/widget/FFloatInput.sim" call "fulltick/widget/FPasswordInput.sim" call "fulltick/widget/FMultilineInput.sim" call "fulltick/widget/FIntInput.sim" call "fulltick/widget/FWindow.sim" ?fulltick.widget call "fulltick/core/FApp.sim" ?fulltick.core block main() input =new FInput("") input.setTitle("Normal : ").setPosition(90,10) finput =new FFloatInput() finput.setPosition(90,45).setTitle("Float : ") iinput =new FIntInput() iinput.setPosition(90,80).setTitle("Int : ") pinput =new FPasswordInput() pinput.setPosition(90,115).setTitle("Password : ") minput =new FMultilineInput() minput.setPosition(90,155).setTitle("Multiline : ") window =new FWindow() window.setTitle("FINPUT(S) DEMO").setSize(500,350) window.addWidget([input,finput,iinput,pinput,minput]) window.show() FApp.run()