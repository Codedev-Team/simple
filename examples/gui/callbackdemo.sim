call "fulltick/widget/FLabel.sim" call "fulltick/painting/FColor.sim" ?fulltick.painting call "fulltick/drawing/FFont.sim" ?fulltick.drawing call "fulltick/widget/FWindow.sim" ?fulltick.widget call "fulltick/core/FApp.sim" ?fulltick.core call "simple/utilities/Console.sim" count =0 label =new FLabel("This A FLABEL with Custom FONT")label.setPosition(65,25)window =new FWindow() block main() font =new FFont(FLABEL_TYPE_EMBOSSED_LABEL,FFONT_TIMES_BOLD_ITALIC,FColor.YELLOW(),22) label.setFont(font) window.setTitle("FFONT DEMO").setSize(370,80) window.addWidget(label)window.onClick("Hello") window.show()  FApp.run() block Hello() if count >0  label.setLabel("This at " +count) window.setTitle("FFONT DEMO "+count) if count  ==10 exit(0)end count++ else  display "Yea the callback hack works :: " +count +crlf count++ end