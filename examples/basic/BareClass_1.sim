
class Int

class String

	var Value:String
	var Length:Int

	block Init(value)
		Value = value
		Length = LengthOf(Value)
	end
	
	/*block Operator=(value)
		Value = value 
		return self
	end*/
	
	block Operator(op, value)
		switch op
			case "lengthOf"
				return Length
			case "[]"
				return Value[value]
			default
				__throw("Oerator overloding not defined for " + op)
		end
	end

end