call "simple/io/Directory.sim" call "simple/io/File.sim" block main() tDir =new Directory("../") loopFolders(tDir) display "TO PATH->TOSTRING : "+tDir.toPath().absolutePath()+crlf display "PARENT DIRECTORY : "+tDir.parent().absolutePath() block loopFolders(dir) tDirs =dir.listDirectories() for a =0 to lengthOf(tDirs) display "FOLDER : "+tDirs[a].absolutePath()+crlf loopFolders(tDirs[a]) end tFiles =dir.listFiles() for a =0 to lengthOf(tFiles) display "FILE : "+tFiles[a].absolutePath()+crlf end